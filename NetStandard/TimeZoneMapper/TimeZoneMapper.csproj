<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.3.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>RobIII</Authors>
    <Company>RobIII</Company>
    <PackageProjectUrl>https://github.com/vijayvepa/TimeZoneMapper</PackageProjectUrl>
    <PackageIconUrl>http://riii.nl/tzmapperlogo</PackageIconUrl>
    <PackageLicenseUrl>https://github.com/RobThree/TimeZoneMapper/blob/master/LICENSE</PackageLicenseUrl>
    <Description>Provides *N*X TimeZone ID to .NET's TimeZoneInfo mappings. Information is based on Unicode Inc's CLDR data files.</Description>
    <PackageReleaseNotes>Ported to .net standard</PackageReleaseNotes>
    <PackageTags>timezone cldr</PackageTags>
  </PropertyGroup>

</Project>
